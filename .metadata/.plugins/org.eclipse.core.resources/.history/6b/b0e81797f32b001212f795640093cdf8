package vendingmachine;

import vending_simu.tag.Bill;

public final class YenBill implements Money, Bill{
	public static enum Kind{
		A_THOUSAND_YEN_BILL {
			@Override
			public YenBill newBill(){
				return new YenBill(1000);
			}
		}, 
		TWO_THOUSAND_YEN_BILL {
			@Override
			public YenBill newBill(){
				return new YenBill(2000);
			}
		}, 
		FIVE_THOUSAND_YEN_BILL {
			@Override
			public YenBill newBill(){
				return new YenBill(5000);
			}
		}, 
		TEN_THOUSAND_YEN_BILL {
			@Override
			public YenBill newBill(){
				return new YenBill(10000);
			}
		}, 
		;
		
		public abstract YenBill newBill();
	}
	private final Yen amount;
	
	YenBill(int amount){
		this.amount= new Yen(amount);
	}
	
	@Override
	public Yen getAmount(){
		return this.amount;
	}

	@Override
	public String getName(){
		return this.amount.toString() + "æœ­";
	}
}
