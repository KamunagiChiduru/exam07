package guava.immutablecollection;

import java.io.File;
import java.io.FileNotFoundException;
import java.io.OutputStream;
import java.io.PrintStream;
import java.io.UnsupportedEncodingException;

public final class SpeedCheck{
	public static void main(String[] args){
		
	}
	
	private static final class DevNull extends PrintStream{
		public DevNull(File file, String csn) throws FileNotFoundException,
				UnsupportedEncodingException{
			super(file, csn);
		}
		
		public DevNull(File file) throws FileNotFoundException{
			super(file);
		}
		
		public DevNull(OutputStream out, boolean autoFlush, String encoding)
				throws UnsupportedEncodingException{
			super(out, autoFlush, encoding);
		}
		
		public DevNull(OutputStream out, boolean autoFlush){
			super(out, autoFlush);
		}
		
		public DevNull(OutputStream out){
			super(out);
		}
		
		public DevNull(String fileName, String csn) throws FileNotFoundException,
				UnsupportedEncodingException{
			super(fileName, csn);
		}
		
		public DevNull(String fileName) throws FileNotFoundException{
			super(fileName);
		}
	}
}
