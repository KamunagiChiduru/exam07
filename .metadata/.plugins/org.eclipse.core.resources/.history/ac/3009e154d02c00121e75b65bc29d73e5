package main;

import main.io.Formattable;

public enum Coin implements Formattable{
	TEN(10), 
	FIFTY(50), 
	HUNDRED(100), 
	FIVE_HUNDRED(500), 
	;
	
	private final int amt;
	private Coin(int amt){
		this.amt= amt;
	}

	@Override
	public String getDisplayText(){
		return String.format("%då††", this.amt);
	}
}
