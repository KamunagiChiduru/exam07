package vendingmachine;

import java.util.Collection;

import vending_simu.tag.Coin;

import com.google.common.collect.ImmutableList;

import customer.Customer;

public class Simulator{
	private final VendingMachine vendingMachine= null;
	private final Customer customer= null;
	
	public void start(){
		VendingMachine activeVendingMachine= this.customer.selectVendingMachine(ImmutableList.of(this.vendingMachine));
		
		Operation nextOperation= this.customer.selectOperation(ImmutableList.<Operation>of());
	}
	
	public static void main(String[] args){
		new Simulator().start();
	}
}
